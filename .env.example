# Private key of the account that will deploy the contract
# IMPORTANT: Never commit this file with your real private key!
PRIVATE_KEY=your_private_key_here

# RPC URLs for different networks
# Get free RPC URLs from Alchemy, Infura, or QuickNode

# Sepolia Testnet (Recommended for testing)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Goerli Testnet
GOERLI_RPC_URL=https://eth-goerli.g.alchemy.com/v2/YOUR_API_KEY

# Ethereum Mainnet (For production)
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
